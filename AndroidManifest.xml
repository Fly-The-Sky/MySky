<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jq"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.jq.ui.DemoApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jq.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jq.ui.BtConfigActivity"
            android:configChanges="orientation|keyboardHidden"
            android:label="Choose Device"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.jq.ui.enforcement_bill.ebMainActivity"
            android:label="@string/title_activity_enforcement_bill" >
        </activity>
        <activity
            android:name="com.jq.ui.enforcement_bill.ebSettingActivity"
            android:label="@string/title_activity_enforcement_bill_setting" >
        </activity>
        <activity
            android:name="com.jq.ui.express.MainExpressActivity"
            android:label="@string/title_activity_main_express" >
        </activity>
        <activity
            android:name="com.jq.ui.card_reader.MainCardReaderActivity"
            android:label="@string/title_activity_main_card_reader" >
        </activity>
        <activity
            android:name="com.jq.ui.express_form.ExpressFormMainActivity"
            android:label="@string/title_activity_express_form_main" >
        </activity>
        <activity
            android:name="com.jq.ui.SerialPortPreferences"
            android:configChanges="orientation|keyboardHidden"
            android:label="Choose Device"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
    </application>

</manifest>